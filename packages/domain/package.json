{
  "name": "@monorepo/domain",
  "version": "1.0.0",
  "description": "Domain layer containing business logic, entities, and domain services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@monorepo/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "rimraf": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "import": "./dist/entities/index.js",
      "require": "./dist/entities/index.js"
    },
    "./value-objects": {
      "types": "./dist/value-objects/index.d.ts",
      "import": "./dist/value-objects/index.js",
      "require": "./dist/value-objects/index.js"
    },
    "./aggregates": {
      "types": "./dist/aggregates/index.d.ts",
      "import": "./dist/aggregates/index.js",
      "require": "./dist/aggregates/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js",
      "require": "./dist/events/index.js"
    },
    "./specifications": {
      "types": "./dist/specifications/index.d.ts",
      "import": "./dist/specifications/index.js",
      "require": "./dist/specifications/index.js"
    },
    "./repositories": {
      "types": "./dist/repositories/index.d.ts",
      "import": "./dist/repositories/index.js",
      "require": "./dist/repositories/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "domain",
    "business-logic",
    "entities",
    "ddd",
    "domain-driven-design"
  ],
  "author": "Monorepo Team",
  "license": "MIT"
}