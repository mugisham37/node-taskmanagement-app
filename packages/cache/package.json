{
  "name": "@taskmanagement/cache",
  "version": "1.0.0",
  "description": "Comprehensive caching package with Redis and memory cache providers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cache",
    "redis",
    "memory-cache",
    "multi-layer",
    "performance",
    "invalidation",
    "warming",
    "strategies",
    "decorators"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^5.7.0",
    "lru-cache": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}