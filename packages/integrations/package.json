    "sharp": "^0.32.6",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google-cloud/translate": "^8.0.2",
    "facebook-nodejs-business-sdk": "^18.0.3",
