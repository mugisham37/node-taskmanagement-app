    "@types/multer": "^1.4.11",
    "@types/paypal-rest-sdk": "^1.7.9",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
