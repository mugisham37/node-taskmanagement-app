{
  "name": "@taskmanagement/integrations",
  "version": "1.0.0",
  "description": "Comprehensive integrations package for email, SMS, push notifications, calendar, file storage, webhooks, payment, analytics, social media, and AI services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest",
